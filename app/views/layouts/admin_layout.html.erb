<!DOCTYPE html>
<html>
<head>
  <title>Globus Group of Institutions - Admin</title>
  <%= stylesheet_link_tag    'flaty', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body data-spy="scroll" data-target="#sidemenu" data-offset="15">
  
  <%= render 'layouts/admin_navigation' %>
 
  <div class="container">
    <div class="row">
       <%= render 'layouts/message' %>
    </div>
    <div class="page-header">
         <h1> Admin_Dashboard</h1>
      </div>
    <div class="row">
      
      <div class="col md-3 col-sm-3">
        <%= render 'layouts/admin_side_nav' %>
      </div>
      
      <div class="col-md-9 col-sm-9">
        <%= yield %>
      </div>
    </div>
    
  </div>

</body>
</html>

