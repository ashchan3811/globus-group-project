<%= render 'layouts/errors', object: @result %>
<%= form_for(@result,html:{class: 'form-horizontal'}) do |f| %>
  
  <div class="form-group">
       <%= f.label(:student_id,'Student', class: 'col-lg-2 col-md-2 col-sm-2 control-label') %>
       <div class="col-lg-10 col-sm-10 col-md-10">
         <%= f.collection_select :student_id, Student.all, :id, :enrollment,{},{ :class => 'form-control'}  %>
       </div>
  </div>
  <div class="form-group">
       <%= f.label(:semester_id,'Semseter', class: 'col-lg-2 col-md-2 col-sm-2 control-label') %>
       <div class="col-lg-10 col-sm-10 col-md-10">
         <%= f.collection_select :semester_id, Semester.all, :id, :name,{},{ :class => 'form-control'}  %>
       </div>
  </div>
  <div class="form-group">
       <%= f.label(:subject_id,'Subject', class: 'col-lg-2 col-md-2 col-sm-2 control-label') %>
       <div class="col-lg-10 col-sm-10 col-md-10">
         <%= f.collection_select :subject_id, Subject.all, :id, :name,{},{ :class => 'form-control'}  %>
       </div>
  </div>
  
  <div class="form-group">
       <%= f.label(:grade_id,'Grade', class: 'col-lg-2 col-md-2 col-sm-2 control-label') %>
       <div class="col-lg-10 col-sm-10 col-md-10">
         <%= f.collection_select :grade_id, Grade.all, :id, :grade,{},{ :class => 'form-control'}  %>
       </div>
  </div>
  
  <div class="form-group">
    <div class="col-sm-10 col-sm-offset-2 col-md-10 col-md-offset-2">
      <%= f.submit class: 'btn btn-danger btn-block'%>
    </div>
  </div>
<% end %>
